{
  "name": "ultra-delete",
  "displayName": "Ultra Cut Delete",
  "description": "Delete or cut entire variables, functions, object keys, or class members with a single keypress",
  "version": "1.7.1",
  "publisher": "henry-dorsett-case",
  "author": {
    "name": "Georgy Sosnin",
    "email": "conspiracy444@yandex.ru"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fushugaku/Ultra-Delete"
  },
  "homepage": "https://github.com/fushugaku/Ultra-Delete",
  "bugs": {
    "url": "https://github.com/fushugaku/Ultra-Delete/issues"
  },
  "keywords": [
    "delete",
    "variable",
    "function",
    "object",
    "class",
    "code-editing",
    "productivity"
  ],
  "categories": [
    "Other",
    "Keymaps"
  ],
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "variableFunctionDeleter.deleteAtCursor",
        "title": "Delete Element at Cursor",
        "category": "Variable Function Deleter"
      },
      {
        "command": "variableFunctionDeleter.cutAtCursor",
        "title": "Cut Element at Cursor",
        "category": "Variable Function Deleter"
      },
      {
        "command": "variableFunctionDeleter.selectFunctionScope",
        "title": "Select Current Function Scope",
        "category": "Variable Function Deleter"
      },
      {
        "command": "variableFunctionDeleter.goToNextMember",
        "title": "Go to Next Member",
        "category": "Variable Function Deleter"
      },
      {
        "command": "variableFunctionDeleter.selectNextMember",
        "title": "Select Next Member",
        "category": "Variable Function Deleter"
      },
      {
        "command": "variableFunctionDeleter.sortMembersAZ",
        "title": "Sort Selected Members A-Z",
        "category": "Variable Function Deleter"
      },
      {
        "command": "variableFunctionDeleter.sortMembersZA",
        "title": "Sort Selected Members Z-A",
        "category": "Variable Function Deleter"
      },
      {
        "command": "variableFunctionDeleter.selectNextMemberAdd",
        "title": "Add Next Member to Selection",
        "category": "Variable Function Deleter"
      },
      {
        "command": "variableFunctionDeleter.moveMemberUp",
        "title": "Move Member Up",
        "category": "Variable Function Deleter"
      },
      {
        "command": "variableFunctionDeleter.moveMemberDown",
        "title": "Move Member Down",
        "category": "Variable Function Deleter"
      }
    ],
    "keybindings": [
      {
        "command": "variableFunctionDeleter.deleteAtCursor",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d",
        "when": "editorTextFocus"
      },
      {
        "command": "variableFunctionDeleter.cutAtCursor",
        "key": "ctrl+shift+x",
        "mac": "cmd+shift+x",
        "when": "editorTextFocus"
      },
      {
        "command": "variableFunctionDeleter.selectFunctionScope",
        "key": "ctrl+shift+s",
        "mac": "cmd+shift+s",
        "when": "editorTextFocus"
      },
      {
        "command": "variableFunctionDeleter.goToNextMember",
        "key": "ctrl+shift+n",
        "mac": "cmd+shift+n",
        "when": "editorTextFocus"
      },
      {
        "command": "variableFunctionDeleter.selectNextMember",
        "key": "ctrl+shift+m",
        "mac": "cmd+shift+m",
        "when": "editorTextFocus"
      },
      {
        "command": "variableFunctionDeleter.sortMembersAZ",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a",
        "when": "editorTextFocus"
      },
      {
        "command": "variableFunctionDeleter.sortMembersZA",
        "key": "ctrl+shift+z",
        "mac": "cmd+shift+z",
        "when": "editorTextFocus"
      },
      {
        "command": "variableFunctionDeleter.selectNextMemberAdd",
        "key": "ctrl+shift+alt+m",
        "mac": "cmd+shift+alt+m",
        "when": "editorTextFocus"
      },
      {
        "command": "variableFunctionDeleter.moveMemberUp",
        "key": "ctrl+shift+up",
        "mac": "cmd+shift+up",
        "when": "editorTextFocus"
      },
      {
        "command": "variableFunctionDeleter.moveMemberDown",
        "key": "ctrl+shift+down",
        "mac": "cmd+shift+down",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "pin": "vsce package && code --install-extension $(ls -t *.vsix | head -n1) --force",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^18.x",
    "@types/vscode": "^1.74.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vscode/test-electron": "^2.3.4",
    "@vscode/vsce": "^2.19.0",
    "eslint": "^8.47.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "typescript": "^5.8.3"
  }
}